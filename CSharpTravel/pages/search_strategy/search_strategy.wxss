/**index.wxss**/
/*整个页面*/
.container{
  height: 1500rpx;
}
/*搜索框*/
.container .search{
  width: 100%;
  height: 65rpx;
  padding: 12.5rpx 12.5rpx 15rpx;
  background: #2A8CE5;
}
/*输入框*/
.container .search-left{
  width: 100%;
  background: #4EA3E7;
  text-align: left;
}
/*输入内容*/
.container .search-left input{
  display: inline-block;
  height: 65rpx;
  font-size: 28rpx;
}
/*提示内容*/
.search-placeholder{
  color: #8CCEFD;
  line-height: 20rpx;
}
/*左侧图案*/
.container .search .search-left image{
  display: inline-block;
  width: 35rpx;
  height: 35rpx;
  padding: 15rpx 15rpx 15rpx 20rpx;
}
